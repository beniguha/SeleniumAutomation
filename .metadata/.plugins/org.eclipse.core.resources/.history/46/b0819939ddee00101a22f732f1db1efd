package testcase;

import org.openqa.selenium.By;
import org.testng.annotations.Test;

public class TestCase2 {
	
	@Test
	public static LoginTest() {
		driver.findElement(By.xpath("//a[@class='zgh-login'][normalize-space()='Sign In']")).click();
		driver.findElement(By.xpath("//input[@id='login_id']")).sendKeys("beni.guha@gmail.com");
		driver.findElement(By.xpath("//button[@id='nextbtn']")).click();
		driver.findElement(By.xpath("//div[@id='otp']")).sendKeys("abcd1234");
		driver.findElement(By.xpath("//button[@id='nextbtn']//span[contains(text(),'Verify')]']")).click();
	}

}
